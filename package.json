{
  "name": "nhl-etl-pipeline",
  "version": "1.0.0",
  "description": "NHL ETL Pipeline",
  "main": "server.ts",
  "scripts": {
    "server": "nodemon backend/server.ts",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "start": "node ./dist/backend/server.js",
    "watch-node": "nodemon dist/backend/server.js",
    "watch-ts": "tsc -w --outDir ./dist",
    "deploy": "git add . && git commit -m Heroku && git push heroku main"
  },
  "keywords": [],
  "author": "Phil Stubbs",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "export-to-csv": "^0.2.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.3",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
